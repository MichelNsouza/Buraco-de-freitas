{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/public/src/(.*)", "dest": "/src/$1" },
    { "src": "/public/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/public/img/(.*)", "dest": "/public/img/$1" },
    { "src": "/public/js/(.*)", "dest": "/public/js/$1" },
    { "src": "/public/uploads/(.*)", "dest": "/public/uploads/$1" },
    { "src": "/public/componentes/(.*)", "dest": "/public/componentes/$1" },
    { "src": "/src/view/(.*)", "dest": "/src/view/$1" },
    { "src": "/src/repositorio/(.*)", "dest": "/src/repositorio/$1" },
    { "src": "/src/model/(.*)", "dest": "/src/model/$1" },
    { "src": "/src/controller/(.*)", "dest": "/src/controller/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/public/api/index.php" }
  ]
}
