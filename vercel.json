{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/uploads/(.*)", "dest": "/uploads/$1" },
    { "src": "/componentes/(.*)", "dest": "/componentes/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
