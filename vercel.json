{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/uploads/(.*)", "dest": "/uploads/$1" },
    { "src": "/componentes/(.*)", "dest": "/componentes/$1" },
    { "src": "/src/view/(.*)", "dest": "/src/view/$1" },
    
    { "src": "/src/repositorio/(.*)", "dest": "/src/repositorio/$1" },
    { "src": "/src/model/(.*)", "dest": "/src/model/$1" },
    { "src": "/src/controller/(.*)", "dest": "/src/controller/$1" },
    { "src": "/img/banner/(.*)", "dest": "/img/banner/$1" },
    { "src": "/img/denuncia/(.*)", "dest": "/img/denuncia/$1" },
    { "src": "/img/nav/(.*)", "dest": "/img/nav/$1" },
    { "src": "/img/ranking/(.*)", "dest": "/img/ranking/$1" },
    { "src": "/img/registros/(.*)", "dest": "/img/registros/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}